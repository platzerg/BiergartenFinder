<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_google_maps_services"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.platzerworld.biergartenfinder.GoogleMapsServicesActivity"
    tools:showIn="@layout/activity_google_maps_services">

    <Button
        android:text="Maps Basic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnMapsBasic"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnMaps"
        android:layout_marginStart="11dp" />

    <Button
        android:text="Maps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnMaps"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
